{
  "schemaVersion": 1,
  "id": "naissancee",
  "version": "${version}",

  "name": "NaissanceE Mod",
  "description": "Mod for NaissanceE custom map.",
  "authors": [
    "Darmo"
  ],
  "contact": {
    "homepage": "http://www.darmo-creations.net",
    "sources": "https://github.com/Darmo117/NaissanceE_mod_1.18"
  },

  "license": "GPL3",
  "icon": "assets/naissancee/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.darmo_creations.naissancee.NaissanceE"
    ],
    "client": [
      "net.darmo_creations.naissancee.ClientInitializer"
    ]
  },

  "mixins": [
    "disable_experimental_settings_warning.mixin.json",
    "disable_hud_bars_in_adventure_mode.mixin.json",
    "disable_hand_rendering_in_adventure_mode.mixin.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17"
  }
}
